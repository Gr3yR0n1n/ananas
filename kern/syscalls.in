#
# Specifies all systems calls. Must be in format:
# <number> { prototype }
#
0	{ void exit(int exitcode); }
1	{ ssize_t read(void* handle, void* buf, size_t len); }
2	{ ssize_t write(void* handle, const void* buf, size_t len); }
3	{ void* map(size_t len); }
4	{ int unmap(void* addr, size_t length); }
5	{ void* open(const char* path, int flags); }
6	{ void close(void* handle); }
7	{ off_t seek(void* handle, off_t offset, int whence); }
8	{ int getdirents(void* handle, void* buf, size_t numitems); }
9	{ void* clone(void* handle); }
10	{ int stat(void* handle, struct stat* sb); }
11	{ int getcwd(char* buf, size_t size); }
12	{ int setcwd(void* handle); }
13	{ handle_event_result_t wait(void* handle, handle_event_t* event); }
14	{ void* summon(void* handle, int flags, const char* args); }
15	{ void* remove(void* handle); }
16	{ off_t truncate(void* handle, off_t length); }
