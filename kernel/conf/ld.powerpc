OUTPUT_ARCH(powerpc)
OUTPUT_FORMAT(elf32-powerpc)
ENTRY(entry)
SECTIONS {
	.text 0x0100000 : { 
		text = .;
		*(.text)
		*(.rodata)
		. = ALIGN(4096);

	}
	.data : {
		*(.data)
		. = ALIGN(4096);
	}
	.bss : {
		*(.bss)
		*(COMMON)
		. = ALIGN(4096);
	}
	__end = . ;
	__traceid_begin = .;
	.traceids : {
		*(.traceids)
	}
	__traceid_end = .;
	.tracenames : {
		*(.tracenames)
	}
}
