OUTPUT_ARCH(i386)
ENTRY(__entry)
SECTIONS {
	/*
	 * Locate the trampoline at the 16MB border; any amd64 system should
	 * have this much memory.
	 */
	.text 0x500000 : {
		text = .;
		*(.text)
		*(.rodata)
		. = ALIGN(4096);
	}
	.data : {
		*(.data)
		. = ALIGN(4096);
	}
	.bss : {
		*(.bss)
		*(COMMON)
		. = ALIGN(4096);
	}
	__end = . ;
}
