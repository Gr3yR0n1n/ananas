OUTPUT_ARCH(i386)
ENTRY(__entry)
SECTIONS {
	.text 0xc0100000 : AT(0x100000) {
		text = .;
		*(.text)
		*(.rodata)
		. = ALIGN(4096);
	}
	.data : {
		*(.data)
		. = ALIGN(4096);
	}
	.bss : {
		*(.bss)
		*(COMMON)
		. = ALIGN(4096);
	}
	__end = . ;
	__traceid_begin = .;
	.traceids : {
		*(.traceids)
	}
	__traceid_end = .;
	.tracenames : {
		*(.tracenames)
	}
}
